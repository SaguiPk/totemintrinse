#:include kv/homepage.kv
#:include kv/psicos.kv
#:include kv/nomecliente.kv
#:include kv/enviomsg.kv
#:import utils kivy.utils

MDFloatLayout:
    canvas:
        Color:
            rgba: utils.get_color_from_hex('#8214EB')
        RoundedRectangle:
            pos: self.pos
            size: self.size
            radius: [0, 0, 0, 0]


    Image:
        pos_hint: {"right": 0.99, "top": 0.4}
        size_hint: 0.50, 0.50
        source: "icones/intrinse3.png"



    ScreenManager
        id: screen_manager
        pos_hint: {"right": 1, "top": 1}
        size_hint: 1, 1

        HomePage:
            name: "homepage"
            id: homepage

        Psicos:
            name: "psicos"
            id: psicos

        NomeCliente:
            name: "nomecliente"
            id: nomecliente

        EnvioMsg:
            name: "enviomsg"
            id: enviomsg



