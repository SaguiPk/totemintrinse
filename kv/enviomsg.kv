<EnvioMsg>:
    FloatLayout

        Label:
            text: " "
            id: id_msg
            markup: True
            font_name: 'Gotham-Rounded-Medium'
            font_size: 50
            pos_hint: {"center_x": .5, "center_y": .5}
            size_hint: 0.1, 0.1
            halign: 'center'
            valign: 'middle'

        MDIconButton:
            icon: "home"
            style: "standard"
            pos_hint: {"right": 0.1 , "top":0.15}
            theme_icon_color: "Custom"
            icon_color: "white"
            theme_font_size: "Custom"
            font_size: "40sp"
            radius: [self.height / 2, ]
            size_hint: None, None
            size: "50dp", "50dp"
            on_release:
                app.casa()
                root.manager.transition.direction = "right"